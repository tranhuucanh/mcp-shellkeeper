src/
*.ts
!*.d.ts
tsconfig.json

# Development files
.github/
scripts/
.vscode/
.idea/
*.log
.DS_Store

# Git files
.git/
.gitignore
.gitattributes

# Tests (if you add them later)
test/
tests/
*.test.js
*.spec.js

# CI/CD
.travis.yml
.circleci/
